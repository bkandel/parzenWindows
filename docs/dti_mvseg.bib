	note = {{PMID:} 15528100},
	keywords = {Algorithms, Brain Mapping, Cerebral Cortex, Fuzzy Logic, Humans, Image Processing, {Computer-Assisted}, Magnetic Resonance Imaging, Reproducibility of Results},
	pages = {997--1012}
},

