@article{thomason_diffusion_2011,
	title = {Diffusion imaging, white matter, and psychopathology},
	volume = {7},
	issn = {1548-5951},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/21219189},
	doi = {10.1146/annurev-clinpsy-032210-104507},
	abstract = {The functional significance of the brain's white matter was not fully appreciated until new imaging methods were developed to visualize fiber pathways and connections in the living brain. Rapid advances in diffusion tensor imaging {(DTI)} have led to substantial insights into human brain development and disease processes and have thrust white matter into the focus of researchers and clinicians alike. The full clinical potential of this relatively new technique remains to be determined, but early indicators suggest that {DTI} will be a significant new technology in mapping mechanisms of human health and disease. Here we review brain changes that have been studied with {DTI} over the human lifespan and findings in a variety of neuropsychiatric disorders. We also suggest future areas where {DTI} is likely to have significant impact.},
	journal = {Annual Review of Clinical Psychology},
	author = {Thomason, Moriah E and Thompson, Paul M},
	month = apr,
	year = {2011},
	note = {{PMID:} 21219189},
	keywords = {Aging, Alzheimer Disease, Anxiety Disorders, Attention Deficit Disorder with Hyperactivity, Autistic Disorder, Brain, Dementia, diffusion tensor imaging, Dyslexia, Humans, Mental Disorders, Mood Disorders, Movement Disorders, Schizophrenia},
	pages = {63--85}
},

